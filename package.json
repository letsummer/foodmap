  "license": "ISC",
  "bugs": {
    "url": "https://github.com/letsummer/foodmap/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.7",
    "webpack-cli": "^5.1.4"
  }
}
